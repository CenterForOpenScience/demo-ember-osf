{
  "name": "demo-ember-osf",
  "version": "0.1.0",
  "description": "A demonstration ember project with OSF brand batteries included",
  "private": true,
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "docs": "yuidoc",
    "check-style": "./node_modules/jscs/bin/jscs .",
    "test": "yarn run check-style && ember test",
    "test:cover": "COVERAGE=true yarn test"
  },
  "repository": "",
  "engineStrict": true,
  "engines": {
    "node": ">=6"
  },
  "author": "Center for Open Science",
  "license": "Apache-2.0",
  "homepage": "https://github.com/abought/demo-ember-osf",
  "devDependencies": {
    "bootstrap-sass": "3.3.7",
    "broccoli-asset-rev": "^2.4.2",
    "coveralls": "^2.11.15",
    "ember-a11y-testing": "0.1.6",
    "ember-ajax": "^2.0.1",
    "ember-bootstrap": "1.0.0-alpha.3",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-bootstrap-sassy": "0.5.5",
    "ember-cli-code-coverage": "^0.3.11",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-qunit": "^2.1.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sass": "6.1.1",
    "ember-cli-sentry": "2.4.2",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-template-lint": "0.5.2",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.8.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-osf": "git+https://github.com/CenterForOpenScience/ember-osf.git#6bd19bf5112cb89e49de99481a1189d72906e162",
    "ember-page-title": "3.1.5",
    "ember-resolver": "^2.0.3",
    "ember-welcome-page": "^1.0.1",
    "jscs": "^3.0.7",
    "loader.js": "^4.0.1",
    "phantomjs-prebuilt": "2.1.14",
    "yuidocjs": "^0.10.2"
  },
  "dependencies": {},
  "jscsConfig": {
    "preset": "airbnb",
    "excludeFiles": [
      "package.json",
      "bower.json",
      "testem.js",
      "ember-cli-build.js",
      "config/environment.js",
      "bower_components",
      "node_modules",
      "coverage",
      "dist",
      "docs",
      "tmp",
      "vendor",
      "app/locales",
      "tests/unit",
      "tests/integration",
      "tests/helpers"
    ],
    "requireSpacesInsideObjectBrackets": false,
    "requireSpacesInsideImportedObjectBraces": false,
    "requireSpacesInAnonymousFunctionExpression": false,
    "requireTrailingComma": false,
    "disallowTrailingComma": false,
    "requirePaddingNewLinesAfterBlocks": false,
    "validateIndentation": 4,
    "requirePaddingNewLinesBeforeLineComments": false,
    "maximumLineLength": false,
    "disallowSpaceBeforeComma": false
  }
}
